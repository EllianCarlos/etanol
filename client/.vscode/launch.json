{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch Extension",
        "type": "extensionHost",
        "request": "launch",
        "program": "${workspaceFolder}/client/src/extension.ts",
        "args": [
          "--extensionDevelopmentPath=${workspaceFolder}"
        ],
        "outFiles": [
          "${workspaceFolder}/client/dist/**/*.js"
        ]
      }
    ]
  }